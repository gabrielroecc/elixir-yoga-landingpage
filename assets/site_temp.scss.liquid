//============ demoaccordion ===============/
.panel-group{
  .panel-default{
    	@include rtl-padding-left(50px);
   	 background-color:transparent;
     border:none;
    @include box-shadow(none);
    .panel-heading{
      border:none;
      background-color:transparent;
    }
  }
  .panel-heading+.panel-collapse .panel-body{
    border:none;
    background-color:transparent;
  }
  .panel-title{
  	font-size:14px;
    .accordion-toggle{
    	color:$theme-color-default;
      	display:block;
      	position:relative;
      	&:before{
      		content:"";
          	@include size(34px,34px);
          	position:absolute;
          	top:-10px;
          	left:-50px;
          	@include box-shadow(0 0 0 34px #000 inset);
          	border-radius:100%;
          	z-index:10;
           @include transition(all 0.5s ease 0s);
      	}
        &:after{
          content:"\f068";
          font-family:$font-icon;
          z-index:11;
          position:absolute;
          top:-10px;
          left:-38px;
          line-height:34px;
          font-weight:normal;
          color:#fff;
        }
      &.collapsed{
        &:after{
          content:"\f067";
          color:$theme-color-default;
        }
        &:before{
          @include box-shadow(0 0 0 2px #000 inset);
        }
      }
      &.in{
        &:after{
          content:"\f068";
          color:#fff;
        }
        &:before{
          @include box-shadow(0 0 0 34px #000 inset);
        }
      }
    } 
  }
}
