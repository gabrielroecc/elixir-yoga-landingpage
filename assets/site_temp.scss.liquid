  #breadcrumbs{
    background: url({{'bg_image_v2.jpg' | asset_url }}) center center fixed repeat transparent;
  }
}
#blogpage{
  	.btn-prenext{
      	a{
			font-family: $font-family-tertiary;
          	font-size: 14px;
          	font-weight: bold;
          	color: $theme-text-color-tertiary;
          	text-transform: uppercase;
          	letter-spacing: 1px;
          	&:hover{
            	color: $theme-text-color;
            }
        }
      	.fa{
        	font-size: 0px;
        }
    }
  	.blog-tags-bottom{
      	font-size: 13px;
      	color: $theme-text-color;
      	margin-top: 10px;
      	.icon-tag{
        	font-size: 0px;
          	position: relative;
          	&:before{
            	content: "\f02c";
              	font-family: $font-icon;
              	font-size: 14px;
              	color: $theme-text-color-tertiary;
            }
        }
    }
  	.blog-heading{
        font-family: $font-family-tertiary;
        font-size: 14px;
        font-weight: bold;
        color: $theme-text-color-secondary;
        letter-spacing: 1px;

    }
}

.blog-social{
	.social-wrap{
		> div {
	    	float: left;
	    }
	}
    .social-heading{
    	padding-right: 12px;
    }
}
.blog-tags{
	padding: 6px 0px;
}
.comment-item {
    padding: 12px 0px;
}
.comment-wrap {
    overflow: hidden;
    background: none repeat scroll 0px 0px #FFF;
    border: $main-border;
    padding: 20px;
    position: relative;
    @include transition(all 0.3s ease-out 0s);
    .comment-meta {
	    border-bottom: $main-border;
	    font-size: 11px;
	    margin: -20px -20px 10px;
	    padding: 10px 20px;
	}
	.comment-content{
      	font-size:13px;
		p{margin: 0px;}
	}
}
.comment-link {
    background: none repeat scroll 0px 0px #F4F4F4;
    border-left: $main-border;
    display: block;
    font-weight: bold;
    margin: -11px -20px -10px 0px;
    padding: 10px 20px;
    @include rtl-float-right;
}
.comment-form{
  	padding-top: 10px;
	.control-label{
		padding-top: 6px;
		text-align: right;
      	display:none;
	}
	.form-group{
      	margin-bottom: 30px;
		&:before,&:after{
			content: "";
			display: table;
		}
		&:after{
			clear: both;
		}
      	&.commentbody{
        	margin-bottom: 10px;
        }
      	.form-control{
        	border: none;
          	box-shadow: none;
          	background: #f4f4f4;
          	font-size: 14px;
        }
	}
	.note_comment{
		font-style: italic;
      	font-size: 13px;
	}
  	.btn{
    	@include rtl-float-right();
      	padding: 6px 20px 8px;
    }
}
.extra-blogs{
	padding-top: 20px;
    h4{
        font-family: $font-family-tertiary;
        font-size: 14px;
        font-weight: bold;
        color: $theme-text-color;
        text-transform: uppercase;
      	padding: 10px 0px;
      	margin: 0px;
    }
	ul{
		margin: 0px;
		padding: 0px;
		li{
			padding: 6px 0px;
			border-bottom: 1px dotted #DDD;
			list-style: outside none none;
          	a{
            	font-size: $theme-font-size - 2;	
            }
		}
	}
}
#blog-listing, #blogpage{
    margin-bottom: 70px;
  	@include rtl-padding-right(10px);
    h1{
      	font-family: $font-family-tertiary;
        font-size: 18px;
      	color: $theme-text-color;
        line-height: 20px;
        margin: 0px;
      	padding: 15px 0px;
    }
    h3{
        font-size: 18px;
        margin: 0px;
      	padding: 0;
      	color: $theme-text-color;
      	a{
        	color: $theme-text-color;
          	&:hover{
            	color: $theme-color-secondary;
            }
        }
    }
  .rte{
    h3{
    margin:10px 0;
    }
    p{
      margin-bottom:10px;
    }
  }
  		.blog-meta{
          	padding: 15px 0px;
          	margin-bottom: 5px;
          	font-style: normal;
          	color: $theme-text-color;
          	line-height: 20px;
      		font-size: 14px;
      		font-weight: normal;
            display: block;
            position: relative;
          	.postby{
            	@include rtl-float-left();
              	@include rtl-padding-right(10px);
            }
          	.blog-tags{
            	padding: 0px 10px;
              	border-style: solid;
              	border-color: #b7b7b7;
              	@include rtl-border-width(0px, 0px, 0px, 1px);
              	@include rtl-float-left();
              	a{
                  	span{
                    	font-style: italic;
                    }
                }
            }
          	.comment-meta{
              	padding: 0px 10px;
              	border-style: solid;
              	border-color: #b7b7b7;
              	@include rtl-border-width(0px, 0px, 0px, 2px);
              	@include rtl-float-left();
            }
        }
      	.rte{
          	.image{
            	margin: 20px 0;
            }
          	img{
            	max-width: 100%;
            }
        }
      	.btn-readmore{
        	border: none;
          	padding: 6px 0px;
          	color: $theme-text-color;
          	font-size: 12px;
          	&:hover, &:focus{
            	background: $nocolor;
              	color: $theme-text-color-tertiary;
            }
        }
}
.blog__post{
  float: left;
  text-align: right;
  padding-right: 35px;
  .blog__post--create{
  	width: 70px;
  	height: 70px;
    background-color:$theme-color-default;
    color: #fff;
    text-align: center;
    font-size: 14px;
    font-weight: 800;
    padding: 17px 15px;
    line-height: 130%;
    text-transform: none;
    display: inline-block;
    span{
      display:block;
    }
  }
}
.blog__right{
  float: left;
  width: 85%;
}
.blog-sidebar{
	.block-sidebar-blog{
        &.block{
          	margin-bottom: 20px;
            ul{
                padding: 0px;
                li{
                  	padding: 10px 0px;
                  	border-bottom: 1px dotted $border-color;
                  	&:last-child{
                    	border-bottom: 0px;
                    }
                    a{
                        padding: 0px;
                        line-height: 18px;
                        font-size: $theme-font-size - 2;
                        text-transform: capitalize;
                      	color: $theme-text-color;
                      	margin-bottom: 8px;
                      	&:hover{
                        	color: $theme-color-secondary;
                        }
                    }
                    time{
                        font-size: $theme-font-size - 2;
                        display: block;
                      	color: $theme-text-color-tertiary;
                        &:last-child{
                            border-bottom: 0px;
                        }
                    }
                }
            }
        }
    }
}
.blog-sidebar{
  	#categories-blog{
      	&.block{
        	ul{
              	&:before,&:after{
                	content: "";
                  	display: table;
                }
              	&:after{
                	clear: both;
                }
                li{
                  	@include rtl-float-left();
                  	@include rtl-padding(0px, 15px, 0px, 0px);
					border-bottom: none;
                  	a{
                    	font-style: italic;
                      	color: $theme-text-color-tertiary;
                      	&:hover{
                        	color: $theme-text-color-secondary;
                        }
                    }
                }
            }
        }
    }
}
#blogsidebar-html{
  	.html_des{
    	font-size: $theme-font-size - 2;
      	color: $theme-text-color;
      	line-height: 22px;
      	margin-bottom: 30px;
    }
}
#blog-listing{
  &.blog--full{
    .rte img{
      width:100%;
    }
  }
  &.blog--masony{
  	h3{
    	font-size:16px;
      line-height:24px;
    }
    .blog__right{
    	width:100%;
    }
    .blog__post{
    	padding-right:15px;
      margin-right:10px;
      border-right :1px solid #b7b7b7;
      span{
      	color:#868686;
      }
    }
    .blog-meta{
      .postby{
        em{
        	font-style:normal;
        }
      }
    }
  }
}

