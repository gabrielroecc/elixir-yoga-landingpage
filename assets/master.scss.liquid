//===== Background Page =====//
{% for i in (1..10) %}
	{% assign idPage = 'page' | append: i | append: '_id' %}
	{% assign bgColor = 'page' | append: i | append: '_bgcolor' %}
	{% assign bgImage = 'page' | append: i | append: '_bgimage' %}
	{% assign bgImageName = 'page' | append: i | append: 'bg.png' %}
	#{{ settings[idPage] }}Apollo{
      	background-color: {{ settings[bgColor] }};
      	{% if settings[bgImage] %}
      		background-image: url({{ bgImageName | asset_url }});
      		background-repeat: no-repeat;
      		
      	{% endif %}
    }
{% endfor %}
