//===== Fancybox =====//
.fancybox-wrap,
.fancybox-skin,
.fancybox-outer,
.fancybox-inner,
.fancybox-image,
.fancybox-wrap iframe,
.fancybox-wrap object,
.fancybox-nav,
.fancybox-nav span,
.fancybox-tmp
{padding: 0;margin: 0;border: 0;outline: none;vertical-align: top;}
.fancybox-wrap {position: absolute;top: 0;left: 0;z-index: 8020;}
.fancybox-opened {z-index: 8030;.fancybox-skin {@include box-shadow(0 10px 25px rgba(0, 0, 0, 0.5));}}
.fancybox-outer, .fancybox-inner {position: relative;}
.fancybox-error {color: $theme-text-color;margin: 0;padding: 15px;white-space: nowrap;}
.fancybox-iframe {display: block;width: 100%;height: 100%;}
.fancybox-image {@extend .fancybox-iframe;max-width: 100%;max-height: 100%;}
#fancybox-loading {position: fixed;top: 50%;left: 50%;display:block;margin-top: -25px;margin-left: -25px;width:50px;height:50px;opacity: 0.8;cursor: pointer;z-index: 8060;background-color:$white;background-image:url({{ 'loading.gif' | asset_url }});@include box-shadow(0 1px 5px rgba(0, 0, 0, 0.5));background-position: center center;background-repeat: no-repeat;border-radius: 4px;}
.fancybox-close {position: absolute;top: 0px;right: -30px;width: 30px;height: 30px;cursor: pointer;z-index: 8040;line-height: 26px;text-align: center;border: 1px solid $border-color;background-color:$white;color:$black;}
.fancybox-close:hover{background-color:$black;color:$white;}
.fancybox-close:before{content: "\f00d";font-family: $font-icon;font-size: 16px;}
.fancybox-nav {position: absolute;top: 0;width: 40%;height: 100%;cursor: pointer;text-decoration: none;background-color: $nocolor;-webkit-tap-highlight-color: rgba(0,0,0,0);z-index: 8040;}
.fancybox-nav span {position: absolute;top: 50%;width:40px;height:40px;margin-top: -20px;cursor: pointer;z-index: 8040;visibility: hidden;text-align:center;line-height:40px;background-color:rgba(0,0,0,0.4);border-radius: 50%;@include box-shadow(0px 1px 3px rgba(0,0,0,0.4));}
.fancybox-nav span:before{content:"";font-family:$font-icon;font-size:20px;}
.fancybox-prev{left: 0;span{&:before{content:"\f104";}}}
.fancybox-next {right: 0;span{&:before{content:"\f105";}}}
.fancybox-nav:hover span {visibility: visible;}
.fancybox-overlay {position: absolute;top: 0;left: 0;overflow: hidden;display: none;z-index: 8010;background-color:rgba(0,0,0,0.3);}
.fancybox-overlay-fixed {position: fixed;bottom: 0;right: 0;}
//===== Owl Carousel =====//
.owl-carousel .owl-wrapper:after {content: ".";display: block;clear: both;visibility: hidden;line-height: 0;height: 0;}
.owl-carousel{display: none;position: relative;width: 100%;-ms-touch-action: pan-y;}
.owl-carousel .owl-wrapper{display: none;position: relative;@include transform(translate3d(0px, 0px, 0px));}
.owl-carousel .owl-wrapper-outer{overflow: hidden;position: relative;width: 100%;direction: ltr !important;}
.owl-carousel .owl-wrapper-outer.autoHeight{@include transition(height 500ms ease-in-out);}
.owl-carousel .owl-item{float: left;}
.owl-controls .owl-page,
.owl-controls .owl-buttons div{cursor: pointer;}
.owl-controls {-webkit-user-select: none;-khtml-user-select: none;-moz-user-select: none;-ms-user-select: none;user-select: none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
.grabbing { cursor:url({{ "grabbing.png" | asset_url }}) 8 8, move;}
.owl-carousel  .owl-wrapper,
.owl-carousel  .owl-item{-webkit-backface-visibility: hidden;-moz-backface-visibility:hidden;-ms-backface-visibility:hidden;@include transform(translate3d(0,0,0));}
.owl-item.loading{min-height: 150px;background: url('{{ "AjaxLoader.gif" | asset_url }}') no-repeat center center;}
.owl-prev, .owl-next {position: absolute;top: 50%;left: 0px;margin-top: -18px;width: 36px;height: 36px;opacity: 0.5;font-size: 0px;color: #292733;text-align: center;text-shadow: 0px 1px 2px rgba(0, 0, 0, 0.6);z-index: 1;border-radius: 2px;border: 1px dashed #898989;background: rgba(241, 250, 255, 0.24) none repeat scroll 0% 0%;line-height: 31px;}
.owl-prev:hover,.owl-prev:focus,.owl-next:hover,.owl-next:focus{outline: 0px;color: #FFF;border-color: #00A9E0;background: #00A9E0 none repeat scroll 0% 0%;text-decoration: none;opacity: 0.9;}
.owl-prev:before,.owl-next:before{content: "\f104";font-family: 'FontAwesome';font-size: 20px;}
.owl-next{right: 0px;left: auto;}
.owl-next:before{content: "\f105";}
$alert-success-bg:            #dff0d8;
$alert-success-text:          #3c763d;
$alert-success-border:        darken(adjust-hue($alert-success-bg, -10), 5%);
$alert-info-bg:               #d9edf7;
$alert-info-text:             #31708f;
$alert-info-border:           darken(adjust-hue($alert-info-bg, -10), 7%);
$alert-warning-bg:            #fcf8e3;
$alert-warning-text:          #8a6d3b;
$alert-warning-border:        darken(adjust-hue($alert-warning-bg, -10), 5%);
$alert-danger-bg:             #f2dede;
