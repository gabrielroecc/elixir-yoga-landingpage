      	visibility:hidden;
      	overflow:hidden;
      	@include opacity(0);
      	@include box-shadow(0px 1px 3px 0px rgba(0,0,0,0.1));
      	&.activeCanvas{
        	position:fixed;
          	left:0px;
          	visibility:visible;
          	@include opacity(1);
        }
      	ul.megamenu{
          	position: absolute;
            top: 0px;
            left: 0px;
            min-height: 100%;
            width: 330px;
          	padding:20px 50px 20px 20px;
