$megamenu-sub-font-size:                {{ settings.apollo_megamenu_fontsizesub }};
$megamenu-sub-bg-color:                 {{ settings.apollo_megamenu_bgsub }};
#apollo-top-menu{
  	font-family:$font-family-tertiary;
	float: right;
  	margin:10px 0 0;
  	padding:0px 25px;
  	text-align: left;
  	@include transition(all 0.2s ease-out);
  	.headerScroll &{
    	margin-top:0px;
    }
}
.apollo-megamenu{
	ul.megamenu{
    	float:none;
      	display:inline-block;
      	vertical-align:middle;
      	margin:0px;
      	padding:0px;
      	text-align:left;
      	>li{
        	position:relative;
          	float:left;
          	margin:0px;
          	padding:0px;
          	> a{
              	display:block;
            	position: relative;
                padding: 20px 25px;
              	font-family:$font-family-tertiary;
                font-size: $megamenu-parent-font-size;
              	line-height:20px;
                text-transform: uppercase;
                color: $megamenu-link-color;
                background-color: transparent;
                font-weight: bold;
                @include transition(all 350ms ease-out);
              	&:hover,&:focus,&:active{                   
                    color: $megamenu-link-hover-color;              
                    background-color: $megamenu-link-bg-hover-color;
                }
            }
        }
    }
  	.dropdown-menu{
      	position: absolute;
      	display: block;
        top: 150%;
        left: 0px;
        height: auto;
        @include opacity(0);
        color: $megamenu-sub-text-color;
        border: 0px;
        min-width: 220px;
      	margin:0px;
        padding: 10px;
        background-color: $megamenu-sub-bg-color;
      	visibility: hidden;
      	z-index:1000;
        @include box-shadow(0px 0px 5px 0px rgba(0, 0, 0, 0.24));
        @include transition(all 0.3s linear 0s);
        li{
            line-height: 20px;
            padding: 8px 0;
          	border-bottom: 1px dashed $border-color;
          	&:last-child{border-bottom: 0px;}
            a{    
                display: block;
                color:$megamenu-sub-link-color;
                padding: 2px 0px;
                font-weight: normal;
                font-size: $megamenu-sub-font-size;
                line-height: 22px;              
                &:hover{
                    background-color: transparent;
                    color: $megamenu-sub-link-hover-color;
                    .fa{
                        color: $megamenu-sub-link-hover-color;
                    }
                }
            }
        }
    }
  	.dropdown{      
        &:hover{
            > .dropdown-menu{                       
                visibility: visible;
                top: 100%;
                @include opacity(1);
                @include transition(all 0.3s linear 0s);
            }
        }       
    }
  	.dropdown-submenu{
        position: relative;
        .dropdown-menu{                     
            top: 0;
            right: auto;
            left: 120%;                 
            position: absolute;
            display: block;
            visibility: hidden;
            @include opacity(0);
            @include transition(all 0.4s ease);
        }
      	
        &:hover{
            > .dropdown-menu{    
                visibility: visible;        
                left: 100%;
                @include opacity(1);
                @include transition(all 0.4s ease);
            }
        }
    }
}
.btnMenuCanvas{
