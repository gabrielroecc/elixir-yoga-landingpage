<section class="tenContainer">
  	<div class="container">
      	<div class="row">
          	<div class="col-xs-12">
              	<div class="boxApollo">
                  	{% assign footer_logo = 'page8.png' %}
                  	<img id="footer_logo" alt="Footer Logo" src="{{ footer_logo | asset_url }}" />
                  	<div class="Description">{{ settings.page8_des }}</div>
                  	{% if settings.landing_social_enable %}
                      <div id="social_block" class="social_block block">
                          <div class="block_content">        
                              <ul class="list-unstyled clearfix">
                                  {% if settings.landing_social_facebook_link != blank %}
                                      <li class="facebook">
                                          <a target="_blank" href="{{ settings.landing_social_facebook_link }}" title="{{ shop.name }} on Facebook" class="btn-tooltip" data-original-title="Facebook">
                                              <i class="fa fa-facebook"></i>
                                             
                                          </a>
                                      </li>
                                  {% endif %}
                                  {% if settings.landing_social_twitter_link != blank %}
                                      <li class="twitter">
                                          <a target="_blank" href="{{ settings.landing_social_twitter_link }}" title="{{ shop.name }} on Twitter" class="btn-tooltip" data-original-title="Twitter">
                                              <i class="fa fa-twitter"></i>
                                             
                                          </a>
                                      </li>
                                  {% endif %}
                                  {% if settings.landing_social_googleplus_link != blank %}
                                      <li class="google-plus">
                                          <a target="_blank" href="{{ settings.landing_social_googleplus_link }}" title="{{ shop.name }} on Google Plus" class="btn-tooltip" data-original-title="Google Plus">
                                              <i class="fa fa-google-plus"></i>
                                              
                                          </a>
                                      </li>
                                  {% endif %}
                                 {% if settings.landing_social_linkedin_link != blank %}
                                      <li class="linked-in">
                                          <a target="_blank" href="{{ settings.landing_social_googleplus_link }}" title="{{ shop.name }} on Linked in" class="btn-tooltip" data-original-title="Linked in">
                                              <i class="fa fa-linkedin"></i>
                                             
                                          </a>
                                      </li>
                                  {% endif %}
                                  {% if settings.landing_social_instagram_link != blank %}
                                      <li class="instagram">
                                          <a target="_blank" href="{{ settings.landing_social_instagram_link }}" title="{{ shop.name }} on Instagram" class="btn-tooltip" data-original-title="Instagram">
                                              <i class="fa fa-instagram"></i>
                                          </a>
                                      </li>
                                  {% endif %}
                              </ul>
                          </div>
                      </div>
                  {% endif %}
              	</div>
          	</div>
      	</div>
  	</div>
</section>